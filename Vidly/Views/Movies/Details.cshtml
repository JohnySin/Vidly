
@model Vidly.Models.Movie

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>@Model.Name</h4>

<ul>
    <li><b>Genre:</b> @Model.Genre.Name</li>
    <li><b>Release Date:</b>@Model.DateReleased</li>
    <li><b>Date Added:</b>@Model.DateAdded</li>
    <li><b>Number In Stok:</b>@Model.NumberInStock</li>
</ul>